{
  "name": "colorful-chalk-logger",
  "version": "0.4.1",
  "description": "a colorful logger, support control output(format | level) through command-line options.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "git@github.com:lemon-clown/colorful-chalk-logger.git",
  "author": "lemon-clown",
  "license": "MIT",
  "scripts": {
    "build": "rimraf lib/ && tsc -b tsconfig.json",
    "postbuild": "rimraf lib/package.json lib/test && mv lib/src/* lib && rimraf lib/src/ lib/**/*.tsbuildinfo",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "logger",
    "colorful",
    "cli options"
  ],
  "files": [
    "lib/",
    ".editorconfig",
    "LICENSE",
    "README.md",
    "package.json",
    "tsconfig.json"
  ],
  "private": false,
  "dependencies": {
    "@types/fs-extra": "^5.0.4",
    "@types/node": "^10.12.18",
    "chalk": "^2.4.2",
    "commander": "^2.19.0",
    "fs-extra": "^7.0.1",
    "moment": "^2.23.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.2.2"
  }
}
